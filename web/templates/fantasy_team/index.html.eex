<section class="page-header">
  <h2>Division <%= @fantasy_league.division %> Fantasy Teams</h2>
</section>

<section class="fantasy-teams-container">
  <%= for fantasy_team <- @fantasy_teams do %>
    <div class="fantasy-team-collection">
      <h5><%= fantasy_team.team_name %></h5>
      <%= render "table.html", fantasy_team: fantasy_team %>
    </div>
  <% end %>
</section>
